@model IEnumerable<TerminatorGym.Models.Membresia>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaVencimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoMembresia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Miembro)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaVencimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoMembresia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Miembro.MiembroId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MembresiaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MembresiaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MembresiaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
